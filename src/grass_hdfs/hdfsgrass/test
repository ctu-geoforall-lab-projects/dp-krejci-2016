#status
curl -i "http://172.17.0.2:50070/webhdfs/v1/?op=GETFILESTATUS"
#ls
curl -i "http://172.17.0.2:50070/webhdfs/v1/foo/?op=LISTSTATUS"
#mkdir
curl -i -X PUT"http://172.17.0.2:50070/webhdfs/v1/foo2?op=MKDIRS"

curl -i "http://localhost:50070/webhdfs/v1/user?user.name=matt&op=GETFILESTATUS"

